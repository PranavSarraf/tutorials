{

  "description": "Demo to show usage of page action css and popup",
  "manifest_version": 2,
  "name": "Page Action Demo",
  "version": "1.0",
  "icons": {
    "48":"icons/off.svg"
  }
  "background": {
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_icon": "icons/off.svg",
    "browser_style": true
  },
  
  "permissions": [
    "activeTab",
    "tabs",
    "notifications"
  ]

}
